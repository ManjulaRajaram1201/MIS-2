               android:layout_centerInParent="true"
               android:background="@drawable/mltn"></Button>

            <TextView
                android:id="@+id/txtview_mlt"
                style="@style/TextMenuName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/btn_mis_mlt"
                android:layout_centerInParent="true"
                android:text="@string/locationtransfer" />
        </RelativeLayout>
